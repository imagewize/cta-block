{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/cta-block",
  "version": "0.1.0",
  "title": "CTA Block",
  "category": "text",
  "icon": "smiley",
  "description": "A Call to Action block with customizable content",
  "supports": {
    "html": false,
    "align": [
      "full",
      "wide"
    ],
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "backgroundColor": {
      "type": "string",
      "default": "sky-600"
    },
    "textColor": {
      "type": "string",
      "default": "white"
    },
    "buttonHoverColor": {
      "type": "string",
      "default": "sky-800"
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "padding": {
            "top": "calc( 0.5 * var(--wp--style--root--padding-right, var(--wp--custom--gap--horizontal)))",
            "bottom": "calc( 0.5 * var(--wp--style--root--padding-right, var(--wp--custom--gap--horizontal)))",
            "left": "var(--wp--style--root--padding-left, var(--wp--custom--gap--horizontal))",
            "right": "var(--wp--style--root--padding-right, var(--wp--custom--gap--horizontal))"
          },
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        },
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|white"
            }
          }
        }
      }
    }
  },
  "styles": [],
  "textdomain": "cta-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}